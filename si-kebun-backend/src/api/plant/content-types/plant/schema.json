    "plantID": {
      "type": "uid",
      "targetField": "name"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "scientificName": {
      "type": "string"
    },
    "shortDescription": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "imageUrl": {
      "type": "string"
    },
    "benefits": {
      "type": "json"
    },
    "careTips": {
      "type": "json"
    },
    "difficulty": {
      "type": "enumeration",
      "enum": [
        "easy",
        "medium",
        "hard"
      ]
    },
    "tags": {
      "type": "json"
    },
    "growthTime": {
      "type": "string"
    },
    "size": {
      "type": "string"
    },
    "plant_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::plant-category.plant-category",
      "inversedBy": "plants"
